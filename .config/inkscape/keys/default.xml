<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<keys
   name="My custom shortcuts">
  <bind
     key="l"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.install"
     display="true" />
  <bind
     key="L"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.install" />
  <bind
     key="k"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.uninstall"
     display="true" />
  <bind
     key="K"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.uninstall" />
  <bind
     key="j"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.view"
     display="true" />
  <bind
     key="J"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.view" />
  <bind
     key="y"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.effects"
     display="true" />
  <bind
     key="Y"
     modifiers="Ctrl,Shift,Alt"
     action="jessyink.effects" />
  <bind
     key="i"
     modifiers="Ctrl,Shift"
     action="ObjectSetClipPath"
     display="true" />
  <bind
     key="I"
     modifiers="Ctrl,Shift"
     action="ObjectSetClipPath" />
  <bind
     key="u"
     modifiers="Ctrl,Shift"
     action="ObjectUnSetClipPath"
     display="true" />
  <bind
     key="U"
     modifiers="Ctrl,Shift"
     action="ObjectUnSetClipPath" />
  <bind
     key="u"
     modifiers="Ctrl,Alt"
     action="AlignVerticalCenter"
     display="true" />
  <bind
     key="U"
     modifiers="Ctrl,Alt"
     action="AlignVerticalCenter" />
</keys>
